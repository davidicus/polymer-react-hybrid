{
  "name": "app-starter",
  "version": "0.0.1",
  "description": "My Project ---",
  "engines": {
    "node": "7.4.0",
    "npm": "4.0.5"
  },
  "main": "index.js",
  "scripts": {
    "clean": "del ./dist",
    "compile:server": "babel server -d dist",
    "compile:webpack": "cross-env NODE_ENV=production webpack -p",
    "compile": "npm run clean && npm run compile:server && npm run compile:webpack",
    "lint:javascript": "eslint ./src ./server/server.js",
    "lint:stylelint": "stylelint './src/**/*.scss' --syntax scss",
    "lint": "npm run lint:stylelint && npm run lint:javascript",
    "start": "cross-env NODE_ENV=development nodemon ./server/server.js --config ./nodemon.json --exec babel-node",
    "start:prod": "cross-env NODE_ENV=production node ./dist/server.js",
    "start:debug": "cross-env NODE_ENV=development nodemon ./server/server.js --config ./nodemon.json --exec babel-node --inspect --debug-brk",
    "test": "jest --coverage & npm run lint",
    "test:a11y": "axe http://localhost:3000/",
    "test:watch": "jest --watch"
  },
  "eslint": {
    "configFile": "./.eslintrc"
  },
  "keywords": [],
  "author": "David Conner",
  "license": "ISC",
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "carbon-components": "^9.5.0",
    "carbon-components-react": "^6.14.2",
    "carbon-icons": "^7.0.7",
    "compression": "^1.6.2",
    "del-cli": "^1.0.0",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "3.0.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.0",
    "axe-cli": "^1.0.2",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.26.1",
    "del-cli": "^1.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.14.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "morgan": "^1.7.0",
    "nock": "^9.0.13",
    "node-sass": "^4.3.0",
    "nodemon": "^1.11.0",
    "polymer-webpack-loader": "^2.0.2",
    "postcss-loader": "^1.2.2",
    "react-axe": "^2.1.2",
    "redux-mock-store": "^1.2.3",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "stylelint": "^7.7.1",
    "stylelint-webpack-plugin": "^0.5.1",
    "sw-precache-webpack-plugin": "^0.11.0",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.9.0"
  }
}
