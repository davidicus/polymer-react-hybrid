{
  "name": "app-starter",
  "version": "0.0.1",
  "description": "My Project ---",
  "main": "index.js",
  "scripts": {
    "clean": "del ./dist",
    "compile:server": "babel server -d dist",
    "compile:webpack": "cross-env NODE_ENV=production webpack -p",
    "compile": "npm run clean && npm run compile:server && npm run compile:webpack",
    "lint:javascript": "eslint ./src ./server/server.js",
    "lint:stylelint": "stylelint './src/**/*.scss' --syntax scss",
    "lint": "npm run lint:stylelint && npm run lint:javascript",
    "start": "cross-env NODE_ENV=development nodemon ./server/server.js --config ./nodemon.json --exec babel-node",
    "start:prod": "cross-env NODE_ENV=production node ./dist/server.js",
    "start:debug": "cross-env NODE_ENV=development nodemon ./server/server.js --config ./nodemon.json --exec babel-node --inspect --debug-brk",
    "test": "jest --coverage & npm run lint",
    "test:a11y": "axe http://localhost:3000/",
    "test:watch": "jest --watch"
  },
  "eslint": {
    "configFile": "./.eslintrc"
  },
  "postcss": {
    "map": {
      "prev": true
    },
    "plugins": {
      "autoprefixer": {}
    }
  },
  "keywords": [],
  "author": "David Conner",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.16.1",
    "carbon-addons-cloud": "^2.1.0",
    "carbon-addons-cloud-react": "^1.2.0",
    "carbon-addons-data-viz-react": "^1.13.0",
    "carbon-components": "^9.1.7",
    "carbon-components-react": "^6.6.4",
    "carbon-icons": "^6.3.2",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "core-decorators": "^0.20.0",
    "express": "^4.15.3",
    "ibm-design-colors": "^2.0.4",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.1",
    "axe-cli": "^3.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.10",
    "del-cli": "^1.0.0",
    "enzyme": "^3.3.0",
    "eslint": "^4.18.2",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "morgan": "^1.7.0",
    "nock": "^9.0.13",
    "node-sass": "^4.7.2",
    "nodemon": "^1.17.3",
    "postcss": "^5.2.6",
    "postcss-loader": "^2.1.3",
    "react-axe": "^2.1.2",
    "redux-mock-store": "^1.5.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.13.1",
